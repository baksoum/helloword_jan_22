pipeline {
    agent any
    tools{
      maven 'M2_HOME'
    }
     stages{
      stage('Build'){
       steps {
         sh 'mvn clean'
         sh 'mvn install'
         sh 'mvn package'
       }
    }
      stage('test'){
       steps{
         sh 'mvn test'
       }
      }
      stage('Deploy'){
       steps{
         echo 'Deploy step'
         sleep 4
       }
      }
      stage('Docker'){
       steps{
         echo'Image step'
       }
      }
      stage('new-dev-branch'){
       steps{
         echo'new branch add (dev) for pull request'
       }
      }
    
    }

}